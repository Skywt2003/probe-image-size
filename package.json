{
  "name": "probe-image-size",
  "version": "8.0.0",
  "description": "Get image size without full download (JPG, GIF, PNG, WebP, BMP, TIFF, PSD, AVIF, HEIC)",
  "keywords": [
    "image",
    "size",
    "jpg",
    "jpeg",
    "ico",
    "gif",
    "png",
    "webp",
    "tiff",
    "bmp",
    "svg",
    "psd",
    "avif",
    "heic"
  ],
  "repository": "nodeca/probe-image-size",
  "license": "MIT",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "src/"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "require": "./dist/index.js",
      "import": "./dist/index.js"
    },
    "./stream": {
      "types": "./dist/stream.d.ts",
      "require": "./dist/stream.js",
      "import": "./dist/stream.js"
    },
    "./sync": {
      "types": "./dist/sync.d.ts",
      "require": "./dist/sync.js",
      "import": "./dist/sync.js"
    },
    "./http": {
      "types": "./dist/http.d.ts",
      "require": "./dist/http.js",
      "import": "./dist/http.js"
    }
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run clean && npm run build",
    "lint": "eslint src --ext .ts",
    "test": "npm run build && mocha",
    "coverage": "npm run build && nyc mocha",
    "report-coveralls": "nyc --reporter=lcov mocha"
  },
  "mocha": {
    "timeout": 5000
  },
  "dependencies": {
    "lodash.merge": "^4.6.2",
    "needle": "^3.3.1",
    "stream-parser": "~0.3.1"
  },
  "devDependencies": {
    "@types/lodash.merge": "^4.6.9",
    "@types/mocha": "^10.0.6",
    "@types/needle": "^3.3.0",
    "@types/node": "^20.11.0",
    "@typescript-eslint/eslint-plugin": "^6.18.1",
    "@typescript-eslint/parser": "^6.18.1",
    "eslint": "^8.56.0",
    "mocha": "^10.2.0",
    "nyc": "^15.1.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
